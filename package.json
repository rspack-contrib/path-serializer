{
  "name": "path-serializer",
  "version": "0.0.1",
  "description": "path-serializer",
  "keywords": ["snapshot", "vitest", "jest", "ci", "test"],
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/path-serializer.git"
  },
  "license": "MIT",
  "author": "sooniter",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/main.cjs",
  "types": "./dist/esm/index.d.mts",
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check --write",
    "prepare": "pnpm run build && simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@microsoft/api-extractor": "^7.47.5",
    "@rslib/core": "file:./scripts/rslib-core-0.0.0.tgz",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.1.0",
    "lodash-es": "^4.17.21",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.2",
    "upath": "^2.0.1"
  },
  "packageManager": "pnpm@9.6.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "pnpm": {
    "overrides": {
      "rsbuild-plugin-dts": "file:./scripts/rsbuild-plugin-dts-0.0.0.tgz"
    }
  }
}
